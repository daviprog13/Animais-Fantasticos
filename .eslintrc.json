{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "spaced-comment": 0,
        "no-param-reassign": 0,
        "eol-last": 0,
        "import/extensions": 0
    }
}